#include<stdio.h>
void add(int a[],int m,int b[],int c){
	int len =sizeof(a)/sizeof(int);
	printf("%d",len);
	 for(int i=0;i<m;i++){
 		printf("%d ",a[i]);
 	}
 	 for(int i=0;i<len-m;i++){
 		printf("%d ",b[i]);
 	}
	
}
int main(){
	int a,b;
	scanf("%d%d",&a,&b);
	int aa[a],bb[b];
	for(int i=0;i<a;i++){
		scanf("%d",&aa[i]);
	}
	for(int i=0;i<b;i++){
		scanf("%d",&bb[i]);	
	}
	int c,d;
	scanf("%d%d",&c,&d);
	
	add(aa,c,bb,d);
	return 0;
	
	
} 
